<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_for_you"
        android:name="com.contrivancenexus.uscientist.ui.foryou.ForYouFragment"
        android:label="For You"
        tools:layout="@layout/fragment_for_you" />

    <fragment
        android:id="@+id/navigation_liked_projects"
        android:name="com.contrivancenexus.uscientist.ui.likedprojects.LikedProjectsFragment"
        android:label="Liked Projects"
        tools:layout="@layout/fragment_liked_projects" />


    <fragment
        android:id="@+id/navigation_home"
        android:name="com.contrivancenexus.uscientist.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.contrivancenexus.uscientist.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />
</navigation>